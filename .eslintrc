{
  "rules": {
    "quotes": [1, "single"],
    "strict": [2, "never"],
    "no-underscore-dangle": 0,
    "dot-notation": [2, {
      "allowPattern": "^[a-z]+(_[a-z]+)+$"
    }],
    "no-undef": 2
  },
  "ecmaFeatures": {
    "generators": true,
    "forOf": true
  },
  "env": {
    "browser": true,
    "node": true
  }
}
